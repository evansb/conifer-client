{
  "name": "conifer-client",
  "version": "0.0.0",
  "description": "Web client for Conifer platform",
  "main": "es5/index.js",
  "jsnext:main": "es6/index.js",
  "typings": "es6/index.d.ts",
  "scripts": {
    "compile": "NODE_ENV=production tsc -p . --declaration && babel es6 --out-dir es5",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "echo Test Coming Soon",
    "dev:basic": "NODE_ENV=development node bin/server.js basic",
    "build:basic": "NODE_ENV=production node bin/build.js basic",
    "clean": "rimraf dist es6 es5 *.log",
    "prepublish": "typings install && npm run lint && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "github.com/evansb/conifer-client"
  },
  "keywords": [
    "IDE",
    "React",
    "Mobx"
  ],
  "author": "Evan Sebastian <evanlhoini@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.22.0",
    "mobx-react-devtools": "^4.2.9",
    "node-sass": "^3.10.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-rc.1",
    "babel-runtime": "^6.11.6",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5"
  }
